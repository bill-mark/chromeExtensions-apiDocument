{
	"manifest_version":2,
	"name":"apiman",
	"version":"0.1",
	"description":"欢迎使用",
	"browser_action":{
		"default_popup":"browser.html",
		"default_title": "接口归纳插件",
		"default_icon":"./icons/logo.png"
	},
	"devtools_page":"devtools.html",
  "permissions":["activeTab","declarativeContent","storage"],
  "background":{
  	"scripts":["js/background.js"],
  	"persistent": false
  }
}